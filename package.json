{
  "name": "qvault",
  "version": "0.0.2",
  "description": "A secure, redundant, and open source secret store",
  "homepage": "https://qvault.io",
  "repository": "https://github.com/Q-Vault/Qvault",
  "keywords": [
    "Qvault",
    "passwords",
    "crypto"
  ],
  "author": {
    "name": "Q Vault",
    "email": "support@qvault.io"
  },
  "main": "main.js",
  "scripts": {
    "release": "yarn build-prod && build",
    "build-prod": "npx webpack --progress --config webpack.config.js --env=production",
    "build": "npx webpack --progress --config webpack.config.js",
    "dev": "yarn build && yarn start",
    "start": "electron .",
    "test": "./node_modules/.bin/mocha --require @babel/register --require @babel/polyfill"
  },
  "build": {
    "appId": "io.qvault.app",
    "productName": "QVault",
    "copyright": "Copyright Â© year QVault",
    "files": [
      "main.js",
      "index.html",
      "dist/*"
    ],
    "directories": {
      "output": "release"
    },
    "publish": "github",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "nsis": {
      "createDesktopShortcut": "always"
    }
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "electron": "^4.0.1",
    "electron-builder": "^20.38.5",
    "electron-updater": "^4.0.6",
    "eslint": "^5.12.0",
    "mocha": "^5.2.0",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "es6-promise": "^4.2.5",
    "isomorphic-fetch": "^2.2.1",
    "jwt-decode": "^2.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "moment": "^2.24.0",
    "random-number-csprng": "^1.0.2",
    "url-loader": "^1.1.2",
    "vue": "^2.5.22",
    "vue-async-computed": "^3.5.1",
    "vue-clipboard2": "^0.2.1",
    "vue-loader": "^15.5.1",
    "vue-qrcode-reader": "^1.3.4",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.28.4"
  }
}
